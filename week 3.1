side_1=int(input())
side_2=int(input())
side_3=int(input())
if(side_1==side_2==side_3):
    print("That's a equilateral triangle")
elif(side_1==side_2!=side_3 or side_1!=side_2==side_3 or side_1==side_3!=side_2):
    print("That's a isosceles triangle")
elif(side_1!=side_2!=side_3):
    print("That's a scalene triangle")
