def factors_of_number(n):
    factors = []
    for i in range(1, int(n**0.5) + 1):
        if n % i == 0:
            factors.append(i)
            if n // i != i:
                factors.append(n // i)
    factors.sort()
    return factors

def find_pth_factor(n, p):
    factors = factors_of_number(n)
    if p <= len(factors):
        return factors[p - 1]
    else:
        return 0

# Input
n = int(input())
p = int(input())

# Output
print(find_pth_factor(n, p))
