def count_frequency(arr):
    frequency_dict = {}
    for item in arr:
        if item in frequency_dict:
            frequency_dict[item] += 1
        else:
            frequency_dict[item] = 1
    
    for key, value in frequency_dict.items():
        print(key, "occurs", value, "times")

# Taking input array from the user
arr = []
n = int(input())
for i in range(n):
    arr.append(int(input()))


count_frequency(arr)
